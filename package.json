{
  "name": "photos",
  "version": "0.0.1",
  "description": "A subdomain of my site for my photography.",
  "keywords": [
    "photography",
    "astro",
    "blog",
    "scottwillsey"
  ],
  "homepage": "https://github.com/scottaw66/photos#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scottaw66/photos.git"
  },
  "license": "MIT",
  "author": "Scott Willsey",
  "scripts": {
    "dev": "export NODE_ENV=development && astro dev",
    "dev:h": "export NODE_ENV=development && astro dev --host",
    "start": "npm run dev",
    "start:h": "npm run dev:h",
    "build": "export NODE_ENV=production && astro build",
    "build:p": "npm run build && npm run preview",
    "build:ph": "npm run build && npm run preview:h",
    "preview": "astro preview",
    "preview:h": "astro preview --host"
  },
  "dependencies": {
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.4.1",
    "@philnash/astro-pagination": "^0.5.0",
    "@tailwindcss/vite": "^4.1.8",
    "astro": "^5.8.2",
    "astro-font": "^1.1.0",
    "astro-pagefind": "^1.8.3",
    "date-fns": "^4.1.0",
    "markdown-it": "^14.1.0",
    "normalize.css": "^8.0.1",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "rehype-accessible-emojis": "^0.3.2",
    "remark-toc": "^9.0.0",
    "sanitize-html": "^2.17.0",
    "tailwindcss": "^4.1.8"
  }
}
